			<HeroSection />
		</section>
		<section id="sobre-nosotros">
			<AboutSection />
		</section>
		<WhyChooseUsSection />
		<section id="tratamiento">
			<TreatmentProcessSection />
		</section>
		<section id="resultados">
			<TestimonialsSection />
		</section>
		<!-- <CTABanner /> -->
		<Footer />
	</main>
</Layout>
